    "/auth/check": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Checks whether the user exists or not",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "usernameOrEmail": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "body": {
                      "type": "object",
                      "properties": {
                        "username": {
                          "type": "string",
                          "example": "riccardo"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/status/UnprocessableEntity"
          },
          "500": {
            "$ref": "#/components/status/InternalServerError"
          }
        }
      }
    },
