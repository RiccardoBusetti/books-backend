  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "status": {
      "Ok": {
        "description": "Ok"
      },
      "UnprocessableEntity": {
        "description": "Unprocessable entity"
      },
      "Unauthorized": {
        "description": "Unauthorized"
      },
      "UnsupportedMediaType": {
        "description": "Unsupported media type"
      },
      "InternalServerError": {
        "description": "Internal server error",
        "$ref": "#/components/responses/ServerError"
      }
    },
    "responses": {
      "ServerError": {
        "description": "An error has occurred in the server",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/AppError"
            }
          }
        }
      },
      "InvalidBodyError": {
        "description": "The body of the request is not valid",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/components/schemas/AppError"
            }
          }
        }
      },
      "InvalidFieldFormatError": {
        "description": "One or more fields are not valid",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/components/schemas/AppError"
            }
          }
        }
      }
    },
    "schemas": {
      "AppError": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "example": "ErrorName"
          },
          "title": {
            "type": "string",
            "example": "Error name"
          },
          "status": {
            "type": "number",
            "example": "000"
          },
          "detail": {
            "type": "string",
            "example": "This error was caused by a component."
          },
          "instance": {
            "type": "string",
            "example": "/example/endpoint"
          }
        }
      },
      "LoginBody": {
        "type": "object",
        "required": [
          "usernameOrEmail",
          "password"
        ],
        "properties": {
          "usernameOrEmail": {
            "type": "string",
            "example": "riccardo | riccardo@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "1234"
          }
        }
      },
      "SignupBody": {
        "type": "object",
        "required": [
          "email",
          "username",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string",
            "example": "riccardo@gmail.com"
          },
          "username": {
            "type": "string",
            "example": "riccardo"
          },
          "password": {
            "type": "string",
            "example": "1234"
          }
        }
      },
      "SellBookBody": {
        "type": "object",
        "required": [
          "isbn",
          "title",
          "description",
          "currency",
          "amount",
          "condition",
          "pictures",
          "locationName"
        ],
        "properties": {
          "isbn": {
